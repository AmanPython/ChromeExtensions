{
  "manifest_version": 3,
  "name": "Google Search Helper",
  "version": "1.0",
  "description": "Open Google and search for a word.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon-16.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    }
  },
  "permissions": [
    "tabs",
    "activeTab",
    "background",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
